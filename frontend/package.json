{
	"name": "@octocloud/validator-frontend",
	"version": "1.0.0",
	"private": true,
	"overrides": {
		"esbuild": "0.25.0",
		"nanoid": "3.3.11",
		"cookie": "0.7.0",
		"@babel/runtime": "7.26.10",
		"brace-expansion": "2.0.2"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check:svelte": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"lint:fix": "prettier --check . && eslint . --fix",
		"format": "prettier . '!./ops' --write --log-level silent",
		"format:check": "prettier . '!./ops' --check",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"typecheck": "tsc --noEmit",
		"check": "npm run check:svelte && npm run lint && npm run format:check"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@types/node": "22.9.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitest/coverage-v8": "1.6.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"fetch-mock": "^9.11.0",
		"msw": "^2.2.14",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"vitest": "1.6.1"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "1.6.3",
		"@octocloud/types": "2.0.0",
		"@sentry/sveltekit": "^7.112.2",
		"@zerodevx/svelte-json-view": "^1.0.9",
		"date-fns": "^3.6.0",
		"highlight.js": "11.9.0",
		"socket.io": "4.7.5",
		"socket.io-client": "4.7.5",
		"vite-plugin-tailwind-purgecss": "0.2.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "2.21.1",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"svelte": "^4.2.7",
		"vite": "5.4.19",
		"typescript": "5.6.3",
		"tailwindcss": "3.4.1",
		"autoprefixer": "10.4.17",
		"postcss": "8.4.35",
		"dotenv": "16.4.5",
		"@skeletonlabs/skeleton": "2.8.0",
		"@skeletonlabs/tw-plugin": "0.3.1",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.10"
	}
}
